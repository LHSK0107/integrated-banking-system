<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.lhsk.iam.domain.admin.model.mapper.AdminMapper">
	
	<select id="findAllLoginHistory" parameterType="String" resultType="LoginHistoryVO">
		SELECT * FROM user_login ORDER BY #{sort} LIMIT ${start}, ${pageSize}
	</select>
	
	<select id="findLoginHistory" parameterType="String" resultType="LoginHistoryVO">
		SELECT * FROM user_login where name = #{name} ORDER BY #{sort} LIMIT ${start}, ${pageSize}
	</select>
	
	
	
</mapper>